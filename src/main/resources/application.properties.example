# ServiceBus Configuration (replace with your actual values)
servicebus.login.sender.connection-string=Endpoint=sb://your-servicebus-namespace.servicebus.windows.net/;SharedAccessKeyName=your-send-policy;SharedAccessKey=your-send-key
servicebus.login.receiver.connection-string=Endpoint=sb://your-servicebus-namespace.servicebus.windows.net/;SharedAccessKeyName=your-listen-policy;SharedAccessKey=your-listen-key
servicebus.queue.name=your-queue-name
servicebus.max-concurrent-calls=1
servicebus.auto-complete=true
servicebus.retry.max-retries=5
servicebus.retry.delay-seconds=2
servicebus.retry.max-delay-seconds=30
servicebus.connection.timeout-minutes=1

# Logging - Suppress verbose Azure SDK logs
logging.level.com.azure=WARN
logging.level.reactor.netty=WARN
logging.level.io.netty=WARN
logging.level.org.example=INFO